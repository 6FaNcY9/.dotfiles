[general]
use-default-font=false
use-scrollbar=false
use-popup=false
start-fullscreen=true
scroll-output=false
set-window-title=true
compat-delete='delete-sequence'
abbreviate-tab-names=false
window-height=50
quick-open-command-line='subl %(file_path)s:%(line_number)s'
gtk-prefer-dark-theme=true
use-login-shell=false
infinite-history=true
display-n=0
quick-open-enable=true
save-tabs-when-changed=false
use-trayicon=false
mouse-display=false
window-losefocus=true
history-size=1000
prompt-on-quit=false
window-halignment=0
start-at-login=true
scroll-keystroke=true
window-width=100
open-tab-cwd=true
window-tabbar=false
window-refocus=true
restore-tabs-startup=false
window-ontop=false
restore-tabs-notify=false
max-tab-name-length=100

[keybindings/local]
focus-terminal-up='<Shift>Up'
focus-terminal-right='<Shift>Right'
split-tab-horizontal='<Shift><Alt>Down'
focus-terminal-left='<Shift>Left'
focus-terminal-down='<Shift>Down'
split-tab-vertical='<Shift><Alt>Right'

[style]
cursor-shape=1
cursor-blink-mode=2

[style/background]
transparency=74

[style/font]
allow-bold=true
style='Fira Code Light 22'
palette='#000000000000:#cdcb00000000:#0000cdcb0000:#cdcbcdcb0000:#1e1a908fffff:#cdcb0000cdcb:#0000cdcbcdcb:#e5e2e5e2e5e2:#4ccc4ccc4ccc:#ffff00000000:#0000ffff0000:#ffffffff0000:#46458281b4ae:#ffff0000ffff:#0000ffffffff:#ffffffffffff:#ffffffffffff:#000000000000'
palette-name='Xterm'
